{
  "name": "Hosts",
  "version": "1.3.4",
  "electronVersion": "1.4.13",
  "description": "Hosts是一个非常简单的host管理工具，轻便的交互体验，快捷的管理方式，开发起来得心应手！",
  "main": "main.js",
  "scripts": {
    "test": "webpack -p",
    "st": "electron .",
    "dev": "webpack -w",
    "r": "webpack --config webpack.releas.js -w",
    "package": "build --dir",
    "dist": "build --mac",
    "ep": "electron-packager ./app Hosts --platform=darwin --arch=x64 --icon=./build/icon.icns --overwrite --out=./ep --version=1.4.13 --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune"
  },
  "author": "puxiao",
  "keywords": [
    "Hosts",
    "hosts"
  ],
  "license": "MIT",
  "build": {
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file",
          "path": "computed path to artifact, do not specify it - will be overwritten"
        }
      ]
    }
  },
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.8.0",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "devtron": "^1.3.0",
    "download-file": "^0.1.5",
    "file-loader": "^0.9.0",
    "github-download": "^0.5.0",
    "heroku": "^0.2.0",
    "immutable": "^3.8.1",
    "is-promise": "^2.1.0",
    "jju": "^1.1.0",
    "json-loader": "^0.5.4",
    "json-parse-helpfulerror": "^1.0.3",
    "lowdb": "^0.13.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.13.0",
    "rc": "^1.1.6",
    "react": "^15.4.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "shelljs": "^0.7.5",
    "steno": "^0.4.4",
    "style-loader": "^0.13.1",
    "underscore-db": "^0.12.0",
    "upx.js": "^1.1.0",
    "url-loader": "^0.5.7",
    "uuid": "^2.0.3",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "electron": "^1.4.13"
  }
}
